<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="263.0" prefWidth="415.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.DeleteRowWindow">
    <children>
        <Label layoutX="111.0" layoutY="17.0" text="Удаление данных">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="43.0" layoutY="73.0" prefHeight="98.0" prefWidth="229.0" text="Введите ID страны, данные которой Вы хотите удалить:" wrapText="true">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="idField" layoutX="260.0" layoutY="108.0" prefHeight="26.0" prefWidth="91.0" promptText="ID" />
        <Button fx:id="submitButton" layoutX="127.0" layoutY="200.0" mnemonicParsing="false" onAction="#submitAction" text="Удалить">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="cancelButton" layoutX="222.0" layoutY="200.0" mnemonicParsing="false" onAction="#cancelAction" text="Отмена">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
